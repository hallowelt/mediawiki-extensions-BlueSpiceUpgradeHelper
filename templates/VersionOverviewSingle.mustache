<div class="version-overview single">
	<div class="version-box">
		<div class="version-term">
			<div>Package:</div>
			<div>{{package}}</div>
		</div>
		<div class="version-number">
			<div>Version:</div>
			<div>{{versionCode}}</div>
		</div>
		<div class="version-support-hours">
			<div>Support hours:</div>
			<div>{{supportHours}}</div>
		</div>
		<div class="version-admin">
			<div>Admin username:</div>
			<div>{{adminUsername}}</div>
		</div>
	</div>
	<button class="button-do-upgrade">Make it so</button>
</div>
