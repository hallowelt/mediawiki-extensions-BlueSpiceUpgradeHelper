<div class="version-overview">
	<div class="version-head">{{version_head}}</div>
	<div class="version-box>
		<div class="version-description">
			{{package_description}}
		</div>
		<div class="version-term">
			<div>{{package_term_label}}:</div> <div>{{package_term}}</div>
		</div>
		<div class="version-support-hours">
			<div>{{package_support_hours_label}}:</div> <div>{{package_support_hours}}</div>
		</div>
		<div class="version-admin">
			<div>{{package_admin_label}}:</div> <div>{{package_admin}}</div>
		</div>
		<div class="version-buttons">
			<button class="version-button-resign">{{package_button_resign}}</button>
			<button class="version-button-extend">{{package_button_extend}}</button>
			<button class="version-button-upgrade">{{package_button_upgrade}}</button>
		</div>
	</div>
</div>
