{
	"name": "BlueSpiceUpgradeHelper",
	"version": "0.0.0",
	"author": [
		"Leonid Verhovskij"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:BoilerPlate",
	"descriptionmsg": "boilerplate-desc",
	"license-name": "MIT",
	"type": "other",
	"AutoloadClasses": {
		"MediaWiki\\Extension\\BlueSpiceUpgradeHelper\\Hooks": "BlueSpiceUpgradeHelper.hooks.php",
