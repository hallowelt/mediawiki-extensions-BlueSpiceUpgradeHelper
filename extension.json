{
	"name": "BlueSpiceUpgradeHelper",
	"version": "3.0.0-alpha",
	"author": [
		"Leonid Verhovskij"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:BoilerPlate",
	"descriptionmsg": "bs-upgrade-helper-desc",
	"license-name": "MIT",
	"type": "other",
	"ConfigRegistry": {
		"boilerplate": "GlobalVarConfig::newInstance"
	},
	"config": {
		"BoilerPlateEnableFoo": {
			"value": true,
			"description": "Whether or not the foo feature is enabled."
		}
	},
	"ExtensionMessagesFiles": {
		"BlueSpiceUpgradeHelperAlias": "BlueSpiceUpgradeHelper.i18n.alias.php"
	},
	"MessagesDirs": {
		"BlueSpiceUpgradeHelper": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.blueSpiceUpgradeHelper.base": {
			"scripts": [
				"ext.blueSpiceUpgradeHelper.js",
				"ext.blueSpiceUpgradeHelper.foo.js"
			],
			"styles": [
				"ext.blueSpiceUpgradeHelper.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"callback": "MediaWiki\\Extension\\BlueSpiceUpgradeHelper\\Hooks\\Main::onRegistration",
	"Hooks": {
		"BSWikiAdminMenuItems": "MediaWiki\\Extension\\BlueSpiceUpgradeHelper\\Hooks\\Main::onBSWikiAdminMenuItems",
		"BeforePageDisplay": "MediaWiki\\Extension\\BlueSpiceUpgradeHelper\\Hooks\\Main::onBeforePageDisplay"
	},
	"GroupPermissions": {
		"sysop": {
			"bluespice-upgradehelper-viewspecialpage": true
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "BlueSpiceUpgradeHelper/modules"
	},
	"SpecialPages": {
		"BlueSpiceUpgradeHelper": "MediaWiki\\Extension\\BlueSpiceUpgradeHelper\\Specials\\UpgradeHelper"
	},
	"load_composer_autoloader": true,
	"manifest_version": 1
}
